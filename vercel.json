{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "OPENSKY_USERNAME": "@opensky_username",
    "OPENSKY_PASSWORD": "@opensky_password"
  },
  "functions": {
    "server.js": {
      "maxDuration": 60
    }
  }
}
